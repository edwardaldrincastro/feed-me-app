{
  "name": "FeedMeMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --config ./.eslintrc.json --ext .js,.jsx,.ts,.tsx",
    "format_all": "prettier --write \"{src,tests}/**/*.{tsx,ts,json,js,jsx}\"",
    "build": "npm run tsc",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/fredAustralia/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "clean:android": "cd android/app && rm -rf build",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "clean:ios": "npm run clean:pods && cd ios && rm -rf build",
    "clean:modules": "rm -rf node_modules && yarn",
    "install:pods": "cd ios && pod install",
    "remove:ios:build": "cd ios && rm -rf build",
    "android": "react-native run-android",
    "android:pixel": "react-native run-android --deviceId=Pixel_3a_API_R",
    "android:avd": "emulator -avd Pixel_3a_API_R",
    "ios": "react-native run-ios",
    "ios:7": "react-native run-ios --simulator=\"iPhone 7 Plus\"",
    "ios:8": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:11": "react-native run-ios --simulator=\"iPhone X\""
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/viewpager": "4.1.6",
    "@react-navigation/native": "5.5.1",
    "@react-navigation/stack": "5.5.1",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-fast-image": "8.3.2",
    "react-native-gesture-handler": "1.6.1",
    "react-native-modal": "11.5.6",
    "react-native-reanimated": "1.9.0",
    "react-native-safe-area-context": "3.1.6",
    "react-native-safe-area-view": "1.1.1",
    "react-native-screens": "2.8.0",
    "react-native-vector-icons": "6.6.0",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "1.0.0",
    "@types/jest": "24.0.24",
    "@types/react-native": "0.62.0",
    "@types/react-native-vector-icons": "6.4.5",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux": "3.6.0",
    "@types/redux-devtools-extension": "2.13.2",
    "@types/redux-thunk": "2.1.0",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-react-native": "3.8.1",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "2.0.4",
    "react-test-renderer": "16.11.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
